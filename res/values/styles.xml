        <item name="themedAddFavIcon">@drawable/ic_addfav</item>
        <item name="themedRemFavIcon">@drawable/ic_remfav</item>
        <item name="themedTopicListIcon">@drawable/ic_topiclist</item>
        <item name="themedPostIcon">@drawable/ic_newpost</item>
        <item name="themedRefreshIcon">@drawable/ic_refresh</item>
